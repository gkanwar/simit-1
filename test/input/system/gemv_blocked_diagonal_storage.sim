element Point
  a : tensor[2](float);
  b : tensor[2](float);
  c : tensor[2](float);
end

extern points  : set{Point};

func dist_a(p : Point) -> (A : tensor[points,points](tensor[2,2](float)))
  A(p,p) = p.a * p.a';
end

export func main()
  A = map dist_a to points reduce +;
  points.c = A * points.b;
end
