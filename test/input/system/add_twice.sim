element Vertex
  a : float;
  b : float;
end

element Edge
  e : float;
end

extern V : set{Vertex};
extern E : set{Edge}(V,V);
extern F : set{Edge}(V,V);

func f(e : Edge, v : (Vertex*2)) -> (A : tensor[V,V](float))
  A(v(0),v(0)) = 0.5 * e.e;
  A(v(0),v(1)) = 0.5 * e.e;
  A(v(1),v(0)) = 0.5 * e.e;
  A(v(1),v(1)) = 0.5 * e.e;
end

export func main()
  B = map f to E reduce +;
  C = map f to F reduce +;
  A = B + C;
  V.a = (A + B + C) * V.b;
end
